{
  version = 1;
  title = "Svelte Chat Log";
  modules = [
    {
      plugin = "source.text";
      data = {
        contents = ''
          eggbug: egg!
          bug!
          eggbug2: egg bug!'';
        language = "text";
      };
      namedSends = { "2" = ["chat-log"]; };
    }
    {
      plugin = "source.lesscss";
      data = {
        contents = ''
          .sender-picture {
            /*
             * if you want to actually use this code,
             * consider uploading your images to cohost
             * and using a cohostcdn URL instead of
             * inlining them into the CSS like this.
             * you might hit the post size limit (~200kB)
             * otherwise!
             */
            &[data-sender="eggbug"] {
              background-image: url("@{eggbug}");
            }
            &[data-sender="eggbug2"] {
              background-image: url("@{eggbug}");
            }
          }

          .chat-log {
            list-style: none;
            padding: 0;
          }
          .chat-message {
            margin: 0;
            margin-top: 0.5em;
            display: flex;
            line-height: 1.2;
            padding: 0;
            @picture-size: 2em;
            @gap: 0.5em;
            gap: @gap;

            &.is-continuation {
              padding-left: @picture-size + @gap;
              margin-top: 0;
            }

            .sender-picture {
              width: @picture-size;
              height: @picture-size;
              border-radius: 50%;
              background-size: cover;
              background-position: center;
              background-repeat: no-repeat;
              flex-shrink: 0;
            }

            .sender-header {
              font-weight: bold;
            }
            .message-contents {
              line-height: 1.5;
            }
          }
        '';
      };
      sends = [4];
    }
    {
      plugin = "source.svelte";
      data = {
        contents = ''
          <script>
            import rawChatLogText from './chat-log';

            const chatLogLines = rawChatLogText.split('\n');
            const chatLogItems = [];
            let lastSender = null;
            for (const line of chatLogLines) {
              const colonPos = line.indexOf(':');

              if (colonPos === -1) {
                chatLogItems.push({
                  type: 'continuation',
                  message: line,
                });
              } else {
                const sender = line.substr(0, colonPos).trim();
                const message = line.substr(colonPos + 1).trim();

                if (sender === lastSender) {
                  chatLogItems.push({
                    type: 'continuation',
                    message,
                  })
                } else {
                  chatLogItems.push({
                    type: 'message',
                    sender,
                    message,
                  });
                  lastSender = sender;
                }
              }
            }
          </script>

          <ul class="chat-log">
            {#each chatLogItems as item}
              {#if item.type == 'message'}
                <li class="chat-message">
                  <div
                    class="sender-picture"
                    data-sender={item.sender}></div>
                  <div class="inner-message">
                    <div class="sender-header">{item.sender}</div>
                    <div class="message-contents">{item.message}</div>
                  </div>
                </li>
              {:else if item.type == 'continuation'}
                <li class="chat-message is-continuation">
                  <div class="message-contents">
                    {item.message}
                  </div>
                </li>
              {/if}
            {/each}
          </ul>'';
      };
      sends = [4];
    }
    {
      plugin = "source.file-data-url";
      data = {
        url = "data:image/svg+xml;base64,";
      };
      namedSends = { "1" = ["eggbug"]; };
    }
    {
      plugin = "transform.style-inliner";
      data = { mode = "attr"; };
      sends = ["output"];
    }
  ];
}