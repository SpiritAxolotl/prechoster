{"modules":[{"id":"550e2d761822f1e9","data":{"contents":"eggbug: egg!\nbug!\neggbug2: egg bug!","language":"text"},"pluginId":"source.text","sends":[],"namedSends":{"87736d8caf60a3f5":["chat-log"]}},{"id":"cb9488669355512c","data":{"contents":".chat-log {\n  /*\n   * if you want to actually use this code,\n   * consider uploading your images to cohost\n   * and using a cohostcdn URL instead of\n   * inlining them into the CSS like this.\n   * you might hit the post size limit (~200kB)\n   * otherwise!\n   */\n  --eggbug: url(\"@{eggbug}\");\n}\n.sender-picture {\n  &[data-sender=\"eggbug\"] {\n    background-image: var(--eggbug);\n  }\n  &[data-sender=\"eggbug2\"] {\n    background-image: var(--eggbug);\n  }\n}\n\n.chat-log {\n  list-style: none;\n  padding: 0;\n}\n.chat-message {\n  margin: 0;\n  margin-top: 0.5em;\n  display: flex;\n  line-height: 1.2;\n  padding: 0;\n  @picture-size: 2em;\n  @gap: 0.5em;\n  gap: @gap;\n  \n  &.is-continuation {\n    padding-left: @picture-size + @gap;\n    margin-top: 0;\n  }\n\n  .sender-picture {\n    width: @picture-size;\n    height: @picture-size;\n    border-radius: 50%;\n    background-size: cover;\n    background-position: center;\n    background-repeat: no-repeat;\n    flex-shrink: 0;\n  }\n\n  .sender-header {\n    font-weight: bold;\n  }\n  .message-contents {\n    line-height: 1.5;\n  }\n}\n"},"pluginId":"source.lesscss","sends":["a13ec312d06590d7"],"namedSends":{}},{"id":"87736d8caf60a3f5","data":{"contents":"<script>\n  import rawChatLogText from './chat-log';\n\n  const chatLogLines = rawChatLogText.split('\\n');\n  const chatLogItems = [];\n  let lastSender = null;\n  for (const line of chatLogLines) {\n    const colonPos = line.indexOf(':');\n\n    if (colonPos === -1) {\n      chatLogItems.push({\n        type: 'continuation',\n        message: line,\n      });\n    } else {\n      const sender = line.substr(0, colonPos).trim();\n      const message = line.substr(colonPos + 1).trim();\n\n      if (sender === lastSender) {\n        chatLogItems.push({\n          type: 'continuation',\n          message,\n        })\n      } else {\n        chatLogItems.push({\n          type: 'message',\n          sender,\n          message,\n        });\n        lastSender = sender;\n      }\n    }\n  }\n</script>\n\n<ul class=\"chat-log\">\n  {#each chatLogItems as item}\n    {#if item.type == 'message'}\n      <li class=\"chat-message\">\n        <div\n          class=\"sender-picture\"\n          data-sender={item.sender}></div>\n        <div class=\"inner-message\">\n          <div class=\"sender-header\">{item.sender}</div>\n          <div class=\"message-contents\">{item.message}</div>\n        </div>\n      </li>\n    {:else if item.type == 'continuation'}\n      <li class=\"chat-message is-continuation\">\n        <div class=\"message-contents\">\n          {item.message}\n        </div>\n      </li>\n    {/if}\n  {/each}\n</ul>"},"pluginId":"source.svelte","sends":["a13ec312d06590d7"],"namedSends":{}},{"id":"d3bf3110095031f6","data":{"url":"data:image/svg+xml;base64,"},"pluginId":"source.file-data-url","sends":[],"namedSends":{"cb9488669355512c":["eggbug"]}},{"id":"a13ec312d06590d7","data":{"mode":"attr"},"pluginId":"transform.style-inliner","sends":["output"],"namedSends":{}}]}
